{
    "version": 2,
    "builds": [
        {
            "src": "app.py",
            "use": "@vercel/python"
        },
        {
            "src": "firebase-config.js",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/view%20quotes.html",
            "status": 301,
            "headers": {
                "Location": "/view_quotes.html"
            }
        },
        {
            "src": "/firebase-config.js",
            "dest": "/firebase-config.js"
        },
        {
            "src": "/(.*)",
            "dest": "app.py"
        }
    ]
}